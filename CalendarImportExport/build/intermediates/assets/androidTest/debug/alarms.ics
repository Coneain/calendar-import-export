BEGIN:VCALENDAR
CALSCALE:GREGORIAN

BEGIN:VEVENT
X-TEST-NAME: 5m before a datetime event
X-TEST-VALUE:reminders=5
DTSTAMP:20000000T000000Z
DTSTART:20150901T123000Z
DTEND:20150901T130000Z
BEGIN:VALARM
TRIGGER:-PT5M
ACTION:DISPLAY
END:VALARM
END:VEVENT

BEGIN:VEVENT
X-TEST-NAME: 5m (in seconds) before a datetime event
X-TEST-VALUE:reminders=5
DTSTAMP:20000000T000000Z
DTSTART:20150901T123000Z
DTEND:20150901T130000Z
BEGIN:VALARM
TRIGGER:-PT300S
ACTION:DISPLAY
END:VALARM
END:VEVENT

BEGIN:VEVENT
X-TEST-NAME: 5m30 before a datetime event
X-TEST-NOTE: Seconds get rounded down in this implementation
X-TEST-VALUE:reminders=5
DTSTAMP:20000000T000000Z
DTSTART:20150901T123000Z
DTEND:20150901T130000Z
BEGIN:VALARM
TRIGGER:-PT300S
ACTION:DISPLAY
END:VALARM
END:VEVENT

BEGIN:VEVENT
X-TEST-NAME: 5m before a datetime event as an absolute time
X-TEST-VALUE:reminders=5
DTSTAMP:20000000T000000Z
DTSTART:20150901T123000Z
DTEND:20150901T130000Z
BEGIN:VALARM
TRIGGER;VALUE=DATETIME:20150901T122500Z
ACTION:DISPLAY
END:VALARM
END:VEVENT

BEGIN:VEVENT
X-TEST-NAME: 5m before a datetime event, AUDIO alarm
X-TEST-VALUE:reminders=5
DTSTAMP:20000000T000000Z
DTSTART:20150901T123000Z
DTEND:20150901T130000Z
BEGIN:VALARM
TRIGGER:-PT5M
ACTION:AUDIO
END:VALARM
END:VEVENT

BEGIN:VEVENT
X-TEST-NAME: 5m and 10m before a datetime event, EMAIL and PROCEDURE alarms
X-TEST-NOTE: Neither type is supported and so both are ignored
X-TEST-VALUE:reminders=
DTSTAMP:20000000T000000Z
DTSTART:20150901T123000Z
DTEND:20150901T130000Z
BEGIN:VALARM
TRIGGER:-PT5M
ACTION:EMAIL
END:VALARM
BEGIN:VALARM
TRIGGER:-PT10M
ACTION:PROCEDURE
END:VALARM
END:VEVENT

BEGIN:VEVENT
X-TEST-NAME: 2 alarms 5m before a datetime event
X-TEST-NOTE: Duplicate alarms are ignored
X-TEST-VALUE:reminders=5
DTSTAMP:20000000T000000Z
DTSTART:20150901T123000Z
DTEND:20150901T130000Z
BEGIN:VALARM
TRIGGER:-PT5M
ACTION:DISPLAY
END:VALARM
BEGIN:VALARM
TRIGGER:-PT5M
ACTION:DISPLAY
END:VALARM
END:VEVENT

BEGIN:VEVENT
X-TEST-NAME: 5m before a datetime event, relative to start
X-TEST-VALUE:reminders=5
DTSTAMP:20000000T000000Z
DTSTART:20150901T123000Z
DTEND:20150901T130000Z
BEGIN:VALARM
TRIGGER;RELATED=START:-PT5M
ACTION:DISPLAY
END:VALARM
END:VEVENT

BEGIN:VEVENT
X-TEST-NAME: 5m after a datetime event starts, relative to start
X-TEST-NOTE: Reminders after the start are ignored
X-TEST-VALUE:reminders=
DTSTAMP:20000000T000000Z
DTSTART:20150901T123000Z
DTEND:20150901T130000Z
BEGIN:VALARM
TRIGGER;RELATED=START:PT5M
ACTION:DISPLAY
END:VALARM
END:VEVENT

BEGIN:VEVENT
X-TEST-NAME: 5m before a datetime event, relative to end
X-TEST-VALUE:reminders=5
DTSTAMP:20000000T000000Z
DTSTART:20150901T123000Z
DTEND:20150901T130000Z
BEGIN:VALARM
TRIGGER;RELATED=END:-PT35M
ACTION:DISPLAY
END:VALARM
END:VEVENT

BEGIN:VEVENT
X-TEST-NAME: 5m after a datetime event ends, relative to end
X-TEST-NOTE: Reminders after the start are ignored
X-TEST-VALUE:reminders=
DTSTAMP:20000000T000000Z
DTSTART:20150901T123000Z
DTEND:20150901T130000Z
BEGIN:VALARM
TRIGGER;RELATED=END:PT5M
ACTION:DISPLAY
END:VALARM
END:VEVENT

BEGIN:VEVENT
X-TEST-NAME: 5m before a date event
X-TEST-VALUE:reminders=5
DTSTAMP:20000000T000000Z
DTSTART:20150901
DTEND:20150901T130000Z
BEGIN:VALARM
TRIGGER:-PT5M
ACTION:DISPLAY
END:VALARM
END:VEVENT

END:VCALENDAR
